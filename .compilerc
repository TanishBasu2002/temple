{
  "env": {
    "development": {
      "presets": [
        ["@babel/env", { "targets": { "electron": "1.6.0" } }],
        "@babel/react",
        "@babel/flow"
      ],
      "plugins": [
        "@babel/plugin-transform-async-to-generator",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-classes",
        "react-hot-loader/babel"
      ],
      "sourceMaps": "inline"
    },
    "test": {
      "presets": [
        ["@babel/env", { "targets": { "electron": "1.6.0" } }],
        "@babel/react",
        "@babel/flow"
      ],
      "plugins": [
        "@babel/plugin-transform-async-to-generator",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-classes",
        "react-hot-loader/babel"
      ],
      "sourceMaps": "inline"
    },
    "production": {
      "presets": [
        ["@babel/env", { "targets": { "electron": "1.6.0" } }],
        "@babel/react",
        "@babel/flow"
      ],
      "plugins": [
        "@babel/plugin-transform-async-to-generator",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-classes"
      ],
      "sourceMaps": "inline"
    }
  }
}
