{
  "name": "temple",
  "productName": "temple",
  "version": "0.1.0",
  "description": "Helper for on-the-fly search-and-copy while coding",
  "author": "cooperka",
  "license": "MIT",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "checkAll": "yarn run lint --fix && yarn run flow && yarn run test --updateSnapshot --watchAll=0",
    "lint": "eslint . --ext .js",
    "flow": "flow",
    "test": "jest",
    "clean": "rm -rf out",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/out/"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "temple"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "auto-launch": "5.0.5",
    "electron-compile": "6.4.4",
    "electron-devtools-installer": "2.2.4",
    "electron-squirrel-startup": "1.0.0",
    "ramda": "0.26.1",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-hot-loader": "4.6.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.33",
    "@electron-forge/maker-deb": "6.0.0-beta.33",
    "@electron-forge/maker-rpm": "6.0.0-beta.33",
    "@electron-forge/maker-squirrel": "6.0.0-beta.33",
    "@electron-forge/maker-zip": "6.0.0-beta.33",
    "babel-cli": "6.26.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-classes": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-cooperka": "0.3.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "flow-bin": "0.92.1",
    "flow-typed": "2.5.1",
    "jest": "24.1.0",
    "react-test-renderer": "16.8.1"
  }
}
